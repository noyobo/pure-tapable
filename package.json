{
  "name": "pure-tapable",
  "version": "1.1.0",
  "description": "tapable 管道流",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/noyobo/pure-tapable.git"
  },
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "tds": "tsc --declaration --emitDeclarationOnly",
    "clear": "rm -rf ./lib",
    "swc:lib": "swc src -d lib  -C module.type=commonjs -C jsc.target=es2015 -C jsc.externalHelpers=true",
    "build": "npm run clear && npm run tds && npm run swc:lib",
    "watch": "npm run swc:lib -- --watch"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@types/jest": "^29.5.11",
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.10",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.7.0",
    "merge": "^2.1.0",
    "standard-version": "^9.5.0",
    "tapable": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.3"
  }
}
